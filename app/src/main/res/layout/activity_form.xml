<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.Form">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingTop="50dp">

            <TextView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:gravity="center"
                android:text="ArtiFex Park"
                android:textSize="30dp" />

            <EditText
                android:id="@+id/setMarqueText"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="renault"
                android:textSize="18dp" />


            <EditText
                android:id="@+id/setModeleText"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="mÃ©gane"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setVersion"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="V0 JIS 7899 265"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setAnnee"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="2022"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setEnergie"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="essence"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setKilometrage"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="298"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setPrix"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="3600.99"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setAnneMiseCircu"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="2020"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setTransmission"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="trans1-essence"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setPremierMain"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="true"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setCategorie"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="berline"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setCarosserie"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="blanche-cool"
                android:textSize="18dp" />

            <EditText
                android:id="@+id/setDescription"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:text="voiture pour des personnes responsable"
                android:textSize="18dp" />

            <Button
                android:id="@+id/buttonPost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="post" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>

